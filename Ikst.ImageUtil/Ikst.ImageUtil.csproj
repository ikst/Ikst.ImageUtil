<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.1;netcoreapp3.1;net5.0</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>ikst</Authors>
    <Company>ikst</Company>
    <PackageProjectUrl>https://github.com/ikst/Ikst.ImageUtil</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ikst/Ikst.ImageUtil</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Bitmap Image Resize</PackageTags>
    <Copyright></Copyright>
    <Description>System.Drawing.Image class extension method + Util class.
Utility classes for image resizing, file output, etc.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>0.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="system.drawing.common" Version="5.0.0" />
  </ItemGroup>

</Project>
